{
  "name": "bundlers",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "pnpm test:bun && pnpm test:vite && pnpm test:webpack",
    "test:bun": "bun build index.html --outdir=bun_dist",
    "test:esbuild": "esbuild src/main.ts --bundle --outdir=esbuild_dist",
