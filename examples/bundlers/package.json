    "esbuild": "^0.20.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "dependenciesMeta": {
    "@typeschema/main": {
      "injected": true
    },
    "@typeschema/zod": {
      "injected": true
    }
  }
